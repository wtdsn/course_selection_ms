{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "./auto-imports.d.ts",
    "components.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/*.d.ts"
  ],
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": [
      "vite/client",
      "element-plus/global",
      "unplugin-icons/types/vue",
      "vue/macros-global"
    ],
  },
  "references": [
    {
      "path": "./tsconfig.config.json"
    }
  ]
}